<div class="box box-info">
  <div class="box-header with-border">
    <h3 class="box-title"><%= user.first_name %> <%= user.last_name%></h3>
  </div><!-- /.box-header -->

    <div class="box-body">
      <dl class="dl-horizontal">
        <dt>Email</dt> <dd> <%= user.email%> </dd>
        <dt>Username</dt> <dd> Username: <%= user.username%> </dd>
      </dl>
   </div>
 </div>


<!-- Info boxes -->
<div class="row">
  <div class="col-md-12">
    <div class="row">
      <div class="col-sm-6">
        <%= render partial: "application_roles"  %>
        <%= render partial: "teams" %>
      </div>
      <div class="col-sm-6">
        <%= render partial: "workspaces" %>
      </div>

    </div>
  </div>
</div>
